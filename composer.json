{
    "name": "yii-extension/user-view-bulma",
    "type": "library",
    "description": "View for extension user with bulma css framework",
    "keywords": [
        "yii3",
        "extension",
        "user",
        "bulma"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "npm-asset/bulma-switch": "^2.0.0",
        "npm-asset/bulma-accordion": "^2.0.1",
        "yii-extension/flash-message-bulma": "@dev",
        "yii-extension/user-settings": "^1.0@dev",
        "yiisoft/assets": "^1.0@dev",
        "yiisoft/form": "^1.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/translator": "^3.0@dev",
        "yiisoft/user": "^3.0@dev",
        "yiisoft/yii-bulma": "^1.0@dev",
        "yiisoft/yii-view": "^3.0@dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3.0",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\Tests\\": "tests"
        }
    },
    "provide": {
        "yii-extension/user-view-implementation": "1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": [
                "config/common/*.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
