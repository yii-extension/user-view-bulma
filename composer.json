{
    "name": "yii-extension/user-view-bulma",
    "type": "library",
    "description": "View for extension user with bulma css framework",
    "keywords": [
        "yii3",
        "extension",
        "user",
        "bulma"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "npm-asset/axios": "^0.21.0",
        "npm-asset/buefy": "0.9.4",
        "npm-asset/bulma-switch": "^2.0.0",
        "npm-asset/vue": "2.6.12",
        "nyholm/psr7": "^1.3.0",
        "yii-extension/flash-message-bulma": "^1.0@dev",
        "yii-extension/fontawesome": "^1.0@dev",
        "yii-extension/parameter-service": "^1.0@dev",
        "yiisoft/assets": "^1.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/form": "^1.0@dev",
        "yiisoft/i18n": "^1.0.0",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/translator-message-php": "^3.0@dev",
        "yiisoft/user": "^3.0@dev",
        "yiisoft/view": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\Tests\\": "tests"
        }
    },
    "provide": {
        "yii-extension/user-view-implementation": "1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-output-dir": "runtime/build/config",
        "config-plugin": {
            "params": "config/params.php",
            "common": [
                "config/common/*.php"
            ],
            "web": [
                "config/web/*.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/yiisoft/translator-message-php"
        }
    ]
}
